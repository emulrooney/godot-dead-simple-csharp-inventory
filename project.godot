; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="EmuInventory"
run/main_scene="res://addons/emuInventory/ExampleScenes/ToolbarExample.tscn"
config/features=PackedStringArray("4.3", "C#", "GL Compatibility")
config/icon="res://addons/emuInventory/BaseComponents/icon.svg"

[display]

window/size/viewport_width=800
window/size/viewport_height=500
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="EmuInventory"

[editor_plugins]

enabled=PackedStringArray("res://addons/emuInventory/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
